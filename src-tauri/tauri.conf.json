{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "palworld-save-pal",
  "version": "0.15.0",
  "identifier": "com.palworld-save-pal.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "palworld-save-pal",
        "width": 1400,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.icns", "icons/icon.ico"]
  }
}
